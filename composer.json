{

    "name": "setastart/galaxia-editor",
    "type": "project",
    "description": "Galaxia Editor is a multilingual web content editor (CMS).",
    "keywords": ["galaxia", "editor", "cms"],
    "homepage": "https://setastart.com/",

    "require": {
        "ext-libxml": "*",
        "ext-mysqli": "*",
        "ext-intl": "*",
        "ext-curl": "*",
        "ext-ctype": "*",
        "ext-vips": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-fileinfo": "*",
        "ext-dom": "*"
    },

    "autoload": {
        "psr-4": {
            "Galaxia\\": "src/shared/core/class/",
            "Galaxia\\RedisCli\\": "src/shared/redis/src/",
            "Galaxia\\FastRoute\\": "src/shared/fastroute/src/"
        },
        "files": [
            "src/shared/core/function/array.php",
            "src/shared/core/function/calendar.php",
            "src/shared/core/function/error.php",
            "src/shared/core/function/gFile.php",
            "src/shared/core/function/gImage.php",
            "src/shared/core/function/input.php",
            "src/shared/core/function/sql.php",
            "src/shared/core/function/text.php",
            "src/shared/core/function/util.php",

            "src/shared/fastroute/src/functions.php"
        ]
    }

}
